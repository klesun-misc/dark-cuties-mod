{
"update_url": "https://clients2.google.com/service/update2/crx",

  "author": "VeRychard",
  "browser_action": {
    "browser_style": true,
    "default_icon": "images/logo_32.png",
    "default_popup": "popup/index.html"
  },
  "content_scripts": [
    {
      "css": [
        "style.css"
      ],
      "js": [
        "jquery-latest.js",
        "content.js"
      ],
      "matches": [
        "https://*.blockchaincuties.com/*"
      ]
    }
  ],
  "description": "Collection of addons and mods for the game Blockchain Cuties",
  "developer": {
    "name": "VeRychard"
  },
  "icons": {
    "128": "images/logo_128.png",
    "16": "images/logo_16.png",
    "32": "images/logo_32.png",
    "48": "images/logo_48.png"
  },
  "manifest_version": 2,
  "name": "Dark Cuties Mods",
  "permissions": [
    "storage",
    "clipboardWrite",
    "https://blockchaincuties.com/",
    "https://dcm.verychard.com/"
  ],
  "version": "0.9.91",
  "web_accessible_resources": [
    "dc_theme.css",
    "dc_bgs.css",
    "pagescript.js"
  ]
}